{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/farmers/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/users/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/posts/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/farmers/(.*)",
      "dest": "api/farmers/$1"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "api/users/$1"
    },
    {
      "src": "/api/posts/(.*)",
      "dest": "api/posts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
